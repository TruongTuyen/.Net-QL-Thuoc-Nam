<!DOCTYPE HTML>
<html>
<head>
    <title>QL Thuốc</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <link href='http://fonts.googleapis.com/css?family=Arimo:400,700' rel='stylesheet' type='text/css'>
    <!--[if lte IE 8]><script src="js/html5shiv.js"></script><![endif]-->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="~/Assets/vendors/jquery/dist/jquery.min.js"></script>

    <script src="~/Assets/js/skel.min.js"></script>
    <script src="~/Assets/js/skel-panels.min.js"></script>
    <script src="~/Assets/js/init.js"></script>
    <script src="~/scripts/custom_js.js"></script>
        
    <link href="~/Assets/css/skel-noscript.css" rel="stylesheet" />
    <link href="~/Assets/css/style.css" rel="stylesheet" />
    <link href="~/Assets/css/style-desktop.css" rel="stylesheet" />
    <link href="~/Assets/css/custom.css" rel="stylesheet" />
    <!--[if lte IE 8]><link rel="stylesheet" href="css/ie/v8.css" /><![endif]-->
    <!--[if lte IE 9]><link rel="stylesheet" href="css/ie/v9.css" /><![endif]-->
    <style>
        .ds_thuoc .4u:nth-child(3n+1){
            clear:left;
        }

    </style>


</head>
<body class="homepage">

    <!-- Header -->
    <div id="header">
        <div class="container">

            <!-- Logo -->
            <div id="logo">
                <h1><a href="#">QL Thuốc Nam - Đông y</a></h1>
                
            </div>

            <!-- Nav -->
            <nav id="nav">
                <ul>
                   
                    <li><a href="~/Caythuoc/Danhsach">Cây thuốc</a></li>
                    <li><a href="~/Baithuoc/Danhsach">Bài thuốc</a></li>
                </ul>
            </nav>
            <div>
                <form class="form" action="~/Baithuoc/Timkiem" method="post" class="form-horizontal custom_search_form">
                    <div class="form-group custom_search_form_wrapper">
                        <input type="text" value="" class="form-control custom_search_form" name="search_str" placeholder="Tìm kiếm bài thuốc" />
                        <input type="submit" name="submit" class="btn btn-primary custom_submit" value="Tìm kiếm" />
                    </div>
                </form>

            </div>
        </div>
    </div>

    <!-- Main -->
    <div id="main">
        <div class="container">
            <div class="row">

                <!-- Content -->
                <div id="content" class="12u skel-cell-important">
                    @RenderBody()
                </div>

            </div>
        </div>
    </div>




    <!-- Copyright -->
    <div id="copyright">
        <div class="container">
            &copy;copyright 2016, Quản lý thuốc nam - đông y
        </div>
    </div>

</body>
</html>